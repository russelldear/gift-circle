{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Cognito": {
      "IncludeErrorDetails": true,
      "MetadataAddress": "https://cognito-idp.us-east-1.amazonaws.com/us-east-1_SxPUKkNxj/.well-known/openid-configuration",
      "RequireHttpsMetadata": false,
      "ResponseType": "code",
      "SaveToken": true,
      "TokenValidationParameters": {
        "ValidateIssuer": true
      }
    }
  },
  "DynamoDb": {
    "RegionEndpoint": "us-east-1",
    "ServiceUrl": ""
  }
}
